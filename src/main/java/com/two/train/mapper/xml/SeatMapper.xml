<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.two.train.mapper.SeatMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.two.train.entity.Seat">
        <id column="id" property="id" />
        <result column="train_id" property="trainId" />
        <result column="seat_grade" property="seatGrade" />
        <result column="seat_num" property="seatNum" />
        <result column="seat_price" property="seatPrice" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, train_id, seat_grade, seat_num, seat_price, gmt_create, gmt_modified
    </sql>

</mapper>
